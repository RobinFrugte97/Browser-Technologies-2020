<%- include('../partials/head') %>
<h1>VRAAG 19</h1>
<form action="/vraag20" method="post" id="vraag19">
    <fieldset>
        <legend>Geef Progressive Web App een cijfer:</legend>
    
        <label>Cijfer van 1 tot 10<input name="PWA cijfer" type="text" inputmode="numeric" pattern="^(?:[1-9]|0[1-9]|10)$" required></label>

    </fieldset>
    <nav>
        <ul>
            <a href="/vraag18">Terug</a>
            <input form="vraag19" type="submit" value="Verder">
        </ul>
    </nav>
    <input type="hidden" name="state" value=19>
    <input type="hidden" name="studentnummer" value=<%= studentnummer %>>
</form>
<section id="progressInfo">
    <label for="voortgang">Vraag 19 van 20</label>
    <progress id="voortgang" max="100" value="95">95%</progress>
</section>
<%- include('../partials/foot') %>